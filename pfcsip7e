******************************************************************
* Kontrolle Eingangsnachricht
******************************************************************
 C100-ANFRAGE-CHECK SECTION.
 C100-00.
**  ---> interne Schnittstelle sichern
     MOVE INTERN-MESSAGE TO TS-INTERN-MESSAGE

**  ---> erstmal termid für ERRLOG aufbereiten
     IF  IMSG-TBMP(41) = 1 AND IMSG-TPTR(41) > 0 AND IMSG-TLEN(41) > 0
         MOVE IMSG-CF(IMSG-TPTR(41):IMSG-TLEN(41)) TO W-TERMNR
         MOVE W-TERMNR TO HEX16
         PERFORM P900-WTHEX
         MOVE HEX8(1:4) TO TERMID of APPL-SPEC-BUF
     END-IF

**  ---> Trace-Nr. schon malmerken
     IF  IMSG-TBMP(11) = 1 AND IMSG-TPTR(11) > 0 AND IMSG-TLEN(11) > 0
         MOVE IMSG-CF(IMSG-TPTR(11):IMSG-TLEN(11)) TO W-TRACENR
     END-IF

**  ---> weitere Felder merken
     MOVE IMSG-MDNR TO MDNR OF TXILOG70
                       MDNR OF TXNLOG70-TS
                       MDNR OF STATION
                       MDNR OF STATIONA
                       MDNR OF ASYNC70
                       MDNR OF ARTMAP
                       W-MDNR

     MOVE IMSG-TSNR TO TSNR OF TXILOG70
                       TSNR OF TXNLOG70-TS
                       TSNR OF STATION
                       TSNR OF STATIONA
                       TSNR OF ASYNC70
                       TSNR OF ARTMAP
                       W-TSNR

     MOVE W-TERMNR  TO TERMNR OF TXILOG70
                       TERMNR OF TXNLOG70-TS
                       TERMNR OF ASYNC70
**
*Identifizierung Indoor oder Outdoor
*C-OPT-OUTDOOR = true, dann Automatenverarbeitung
*

     IF IMSG-TBMP(25) = 1
        SET C-OPT-OUTDOOR OF CSYS-FLAG TO TRUE
     ELSE
        SET C-POS-INDOOR  OF CSYS-FLAG TO TRUE
     END-IF

**Zunächst den MAC prüfen, sofern vorhanden
     SET MAC-NO TO TRUE
     IF  IMSG-TBMP(64) = 1
         SET MAC-YES TO TRUE
         SET W66-EC  TO TRUE
         PERFORM F910-MAC-PRUEFEN
         IF  W-AC > ZERO
             SET ENDE TO TRUE
             EXIT SECTION
         END-IF
     END-IF

**  ---> Station zum Terminal lesen
     PERFORM S160-SELECT-STATION
     IF  ENDE
         EXIT SECTION
     END-IF
**  ---> Nachrichtentyp bestimmen (Buchung)
     MOVE IMSG-NTYPE    TO W-NTYPE

**  ---> Abwicklungs-KZ bestimmen (Zahlung/Gutschrift)
     MOVE IMSG-CF(IMSG-TPTR(03):IMSG-TLEN(03)) TO W-ABWKZ
     MOVE IMSG-CF(IMSG-TPTR(03) + 2:4)         TO W-BELEGNR

**  ---> Betrag aufbereiten und merken
     MOVE IMSG-CF(IMSG-TPTR(04):IMSG-TLEN(04)) TO W-BETRAG
     COMPUTE W18-BETRAG = W-BETRAG / 100

*G.01.05 - Anfang
*
*--> ZP-VERKAUF - Berechnung jetzt über Modul ZPVERK
*
**  ---> Verkaufszeitpunkt aufbereiten (20 + BMP13 + BMP12)
*     MOVE IMSG-CF(IMSG-TPTR(12):IMSG-TLEN(12)) TO D-NUM6
*     MOVE IMSG-CF(IMSG-TPTR(13):IMSG-TLEN(13)) TO D-NUM12
*     COMPUTE D-NUM12      = D-NUM12 * 1000000
*     COMPUTE W-ZP-VERKAUF = (10000000000 * TAL-JHJJ of TAL-TIME-D)
*                          + D-NUM12 + D-NUM6


*--> Werte fuer ZP_VERKAUF,  Format:JJJJMMTThhmmss
*--> Z-ZPVERKAUF-RC = 0 ==> alles ok, ZPVERKAUF-TXILOG70 berechnet
*                   > 0 ==> Fehler,   ZPVERKAUF-TXILOG70 = 0
*                   = 1 ==> BMP 12 oder 13 nicht numerisch
*                   = 2 ==> Fehler bei der Berechnung mit HCTIME

      MOVE 1                                    TO ZPVERKAUF-RC
      MOVE TAL-TIME-N                           TO ZPVERKAUF-FEPTALZEIT
      MOVE IMSG-CF(IMSG-TPTR(12):IMSG-TLEN(12)) TO ZPVERKAUF-BMP12
      MOVE IMSG-CF(IMSG-TPTR(13):IMSG-TLEN(13)) TO ZPVERKAUF-BMP13
      MOVE 0                                    TO ZPVERKAUF-TXILOG70
      CALL "ZPVERK" USING ZPVERKAUF-IFC
      MOVE ZPVERKAUF-TXILOG70                   TO W-ZP-VERKAUF

      EVALUATE ZPVERKAUF-RC
**      ---> OK
          WHEN ZERO   CONTINUE

**      ---> Numerik Fehler
          WHEN 1      MOVE "Info: Fehler bei der Berechnung ZP-VERKAUF"
                           TO DATEN-BUFFER1
                      MOVE "BMP 12 oder 13 nicht numerisch"
                           TO DATEN-BUFFER2
                      MOVE "In TXILOG70.ZP-VERKAUF wird Null eingetragen."
                           TO DATEN-BUFFER3
                      PERFORM Z002-PROGERR

**      ---> HCTIME Fehler
          WHEN 2      MOVE "Info: Fehler bei der Berechnung ZP-VERKAUF"
                           TO DATEN-BUFFER1
                      MOVE "Zeitberechnung über das Modul HCTIME fehlerhaft "
                           TO DATEN-BUFFER2
                      MOVE "In TXILOG70.ZP-VERKAUF wird Null eingetragen."
                           TO DATEN-BUFFER3
                      PERFORM Z002-PROGERR
      END-EVALUATE
*G.01.05 - Ende


**  ---> holen Cardid
     PERFORM D900-GET-CARDID
     IF  W-AC NOT = ZERO
         EXIT SECTION
     END-IF

**  ---> holen zuständigen Keynamen
     PERFORM D200-FIX-KEY
**  ---> setzen Keyname für diese Transaktion
     MOVE TK-KEYNAME (C4-I1) TO W-KEYNAME
     MOVE TK-HEXISO (C4-I1)  TO W-ISOGEN-VERS

**  ---> Handeingabe erlaubt? Dann ist BMP35 Pflicht
**  ---> S-ROUTKZ und S-CARDID sind schon gesetzt
     MOVE 200  TO S-ISONTYP
     MOVE "TS" TO S-KZ-MSG
     MOVE 35   TO S-BMP
     MOVE 1    TO S-LFDNR
     PERFORM U300-SEARCH-TAB
     IF  PRM-FOUND
         IF  T-KZ-ABWEICHUNG (T-AKT-IND) = "1"
             IF  not IMSG-TBMP(35) = 1
                 SET ERF-ERROR TO TRUE
                 MOVE 30  TO W-AC
                 MOVE 2201 TO ERROR-NR of GEN-ERROR
                 MOVE "BMP35 fehlt@" TO DATEN-BUFFER1
                 MOVE "Handeingabe nicht erlaubt" TO DATEN-BUFFER2
                 PERFORM Z002-PROGERR
                 EXIT SECTION
             END-IF
         END-IF
     END-IF

**  ---> Prüfen BMP35 , wenn nicht vorhanden, dann keine Verarbeitung
     MOVE ZERO TO C4-ANZ
     MOVE 1    TO C4-PTR
     MOVE SPACES     TO W-DELIM-TABELLE
     MOVE LOW-VALUES TO W-COUNT-TABELLE

     IF  IMSG-TBMP(35) NOT = 1
*Fehler:BMP35 nicht vorhanden, keine Prüfung möglich
         SET ERF-ERROR TO TRUE
         MOVE 30  TO W-AC
         MOVE 2201 TO ERROR-NR of GEN-ERROR
         MOVE "BMP35 (Spur2-Daten) ungültig" TO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
     ELSE
*Spur 2 ist vorhanden
         SET ERF-SPUR2 TO TRUE
         MOVE IMSG-CF(IMSG-TPTR(35):IMSG-TLEN(35)) TO W-SPUR2

         UNSTRING W-SPUR2
         delimited by "D" or "="
             INTO W-TEILSTRING (1)  delimiter in W-DELIM (1)
                                    count     in W-COUNT (1)
                  W-TEILSTRING (2)  delimiter in W-DELIM (2)
                                    count     in W-COUNT (2)
             with pointer C4-PTR
             tallying in  C4-ANZ
         END-UNSTRING

         MOVE W-TEILSTRING (1) TO W-KANR
         MOVE W-COUNT (1)      TO W-KANR-LEN
         IF  C4-ANZ > 1
             MOVE W-TEILSTRING (2) (1:4) TO W-ABL
         ELSE
             MOVE ZEROES                 TO W-ABL
         END-IF

*Uebernehmen Spur2 in Kartenprofil (je nach Kartenart)
         EVALUATE TRUE
             WHEN DKV
                  MOVE W-SPUR2                 TO W-TRACK2-DKV
                  MOVE W-DKV-RSTCODE           TO RST-CODE OF WGRSTPRF
             WHEN NOVOFLEET
                  MOVE W-SPUR2                 TO W-TRACK2-NOVOFLEET
                  MOVE W-NVF-RSTCODE           TO RST-CODE OF WGRSTPRF

             WHEN OTHER
                  SET ERF-ERROR TO TRUE
                  MOVE 30  TO W-AC
                  MOVE 2904 TO ERROR-NR of GEN-ERROR
                  MOVE "DKV oder NOVEFLEET" TO DATEN-BUFFER1
                  PERFORM Z002-PROGERR
                  EXIT SECTION
         END-EVALUATE
     END-IF

**  ---> Erfassungsart (BMP22)
     MOVE IMSG-CF(IMSG-TPTR(22) + 1:2) TO W-ERFASSUNGS-ART
     IF  (W-ERFASSUNGS-ART = "01" and not ERF-MANUELL)
     or  (W-ERFASSUNGS-ART = "02" and not ERF-SPUR2)
         SET ERF-ERROR TO TRUE
         MOVE 30  TO W-AC
         MOVE 2201 TO ERROR-NR of GEN-ERROR
         MOVE "Kombination aus:@" TO DATEN-BUFFER1
         MOVE "BMP22 und BMP2,14,35 nicht korrekt" TO DATEN-BUFFER2
         PERFORM Z002-PROGERR
         EXIT SECTION
     END-IF

* Transaktionen aus Rumänien nicht erlaubt im SIP-Verfahren
     IF IMSG-CF (IMSG-TPTR(49):IMSG-TLEN(49)) = "0946"
        MOVE 02 TO W-AC
        EXIT SECTION
     END-IF

**  ---> ggf. BMP25

     IF  IMSG-TBMP(37) = 1
         MOVE 30  TO W-AC
         MOVE 2201 TO ERROR-NR of GEN-ERROR
         MOVE "Feld zuviel (BMP37)@" TO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
     END-IF

**  ---> Währungs-KZ könnte hier gegen Tabelle WKZKURS geprüft werden
     MOVE IMSG-CF (IMSG-TPTR (49) + 1:3) TO W-WKZ
     MOVE W-WKZ TO WKZ-WKZ
     SET WKZ-CMD-WKZ-A TO TRUE
     PERFORM M170-CALL-SYSAWKZ
     IF  WKZ-ERR
         EXIT SECTION
     END-IF

**  ---> Artikeldaten müssen vorhanden sein
     IF  IMSG-TBMP(63) = 1
         MOVE IMSG-CF(IMSG-TPTR(63) + 32:1)  TO HEX8
         PERFORM P910-WTUNHEX
*G.06.03 - Anfang - BMP63 ohne Artikel
*        IF  HEX16(1:2) = "00"
**
         IF  IMSG-TLEN(63) = 33
*G.06.03 - Ende - BMP63 ohne Artikel

             MOVE 30   TO W-AC
             MOVE 2201 TO ERROR-NR of GEN-ERROR
             MOVE "Autorisierung OHNE Artikel@" TO DATEN-BUFFER1
             PERFORM Z002-PROGERR
             EXIT SECTION
         END-IF
     END-IF

** BMP63 auseindanderfieseln für Restriktionsprüfung
** Pruefen, ob in BMP63 etwas sinnvolles steht

     MOVE ALL SPACES                            TO WEAT-STRUCT
     MOVE IMSG-CF(IMSG-TPTR(63):IMSG-TLEN(63))  TO WEAT-STRUCT
     MOVE ZEROS                                 TO W-BUFFER-LEN

*---> Entpacken Infodaten

     MOVE WEAT-KM           TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:6) NUMERIC
        MOVE HEX16(1:6)     TO UP-KM
     ELSE
        MOVE ZEROES         TO UP-KM
     END-IF

     ADD  3                 TO W-BUFFER-LEN

     MOVE WEAT-KFZNR TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:4) NUMERIC
        MOVE HEX16(1:4)     TO UP-KFZNR
     ELSE
        MOVE ZEROES         TO UP-KFZNR
     END-IF

     ADD  2                 TO W-BUFFER-LEN

     MOVE WEAT-FAHRER TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:4) NUMERIC
        MOVE HEX16(1:4)     TO UP-FAHRER
     ELSE
        MOVE ZEROES         TO UP-FAHRER
     END-IF

     ADD  2                 TO W-BUFFER-LEN

     MOVE WEAT-ERFASSUNG TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:6) NUMERIC
        MOVE HEX16(1:6)     TO UP-ERFASSUNG
     ELSE
        MOVE ZEROES         TO UP-ERFASSUNG
     END-IF

     ADD  3                 TO W-BUFFER-LEN

     MOVE WEAT-INFO         TO UP-INFO
     ADD  20                TO W-BUFFER-LEN

     MOVE WEAT-EINGABE TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:2) NUMERIC
        MOVE HEX16(1:2)     TO UP-EINGABE
     ELSE
        MOVE ZERO           TO UP-EINGABE
     END-IF
     ADD  1                 TO W-BUFFER-LEN

*Filler PIC X(01) wird berücksichtigt
     ADD 1                  TO W-BUFFER-LEN

*Entpacken und pruefen Counter

     MOVE WEAT-COUNTER TO HEX8
     ENTER TAL "WT^UNHEX" USING HEX8, HEX16
     IF HEX16(1:2) NUMERIC
        MOVE HEX16(1:2)     TO UP-COUNTER
        ADD  1              TO W-BUFFER-LEN
     ELSE
        MOVE 30             TO W-AC
        STRING "Numericfehler BMP63 - Counter=", HEX16(1:2)
        DELIMITED BY SIZE INTO DATEN-BUFFER1
        PERFORM Z002-PROGERR
        EXIT SECTION
     END-IF

     IF UP-COUNTER > 20
        STRING "Zu viele Artikel: " UP-COUNTER
        DELIMITED BY SIZE INTO DATEN-BUFFER1
        PERFORM Z002-PROGERR
        MOVE 30              TO W-AC
        EXIT SECTION
     END-IF

*Entpacken Artikel

     PERFORM VARYING C4-I1 FROM 1 BY 1 UNTIL C4-I1 > UP-COUNTER

      MOVE WEAT-PROD-CODE(C4-I1) TO HEX8
      ENTER TAL "WT^UNHEX" USING HEX8, HEX16
      IF HEX16(1:6) NUMERIC
         MOVE HEX16(1:6)     TO UP-PROD-CODE(C4-I1)
         ADD  3              TO W-BUFFER-LEN
      ELSE
         MOVE C4-I1          TO D-NUM4
         MOVE 30             TO W-AC
         STRING "Numericfehler BMP63 - Artikelpos=", D-NUM4, " - Warengruppe"
         DELIMITED BY SIZE INTO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
      END-IF

      MOVE WEAT-ART-EINZELPREIS(C4-I1) TO HEX8
      ENTER TAL "WT^UNHEX" USING HEX8, HEX16
      IF HEX16(1:8) NUMERIC
         MOVE HEX16(1:8)     TO UP-ART-EINZELPREIS(C4-I1)
         ADD  8              TO W-BUFFER-LEN
      ELSE
         MOVE C4-I1          TO D-NUM4
         MOVE 30             TO W-AC
         STRING "Numericfehler BMP63 - Einzelpreis=", D-NUM4, " - Einzelpreis"
         DELIMITED BY SIZE INTO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
      END-IF

      MOVE WEAT-QNT(C4-I1) TO HEX8
      ENTER TAL "WT^UNHEX" USING HEX8, HEX16
      IF HEX16(1:6) NUMERIC
         MOVE HEX16(1:6)     TO UP-QNT(C4-I1)
         ADD  3              TO W-BUFFER-LEN
      ELSE
         MOVE C4-I1          TO D-NUM4
         MOVE 30             TO W-AC
         STRING "Numericfehler BMP63 - Artikelpos=", D-NUM4, " - Menge"
         DELIMITED BY SIZE INTO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
      END-IF

      MOVE WEAT-AMNT(C4-I1) TO HEX8
      ENTER TAL "WT^UNHEX" USING HEX8, HEX16
      IF HEX16(1:10) NUMERIC
         MOVE HEX16(1:10)     TO UP-AMNT(C4-I1)
         ADD  5               TO W-BUFFER-LEN
      ELSE
         MOVE C4-I1          TO D-NUM4
         MOVE 30             TO W-AC
         STRING "Numericfehler BMP63 - Artikelpos=", D-NUM4, " - Betrag"
         DELIMITED BY SIZE INTO DATEN-BUFFER1
         PERFORM Z002-PROGERR
         EXIT SECTION
      END-IF

      EVALUATE WEAT-VZ(C4-I1)

         WHEN " "    MOVE WEAT-VZ(C4-I1)   TO UP-VZ(C4-I1)
                     ADD  1                TO W-BUFFER-LEN
         WHEN "-"    MOVE WEAT-VZ(C4-I1)   TO UP-VZ(C4-I1)
                     ADD  1            TO W-BUFFER-LEN
         WHEN OTHER  MOVE C4-I1        TO D-NUM4
                     STRING "Vorzeichen ungueltig: " WEAT-VZ(C4-I1)
                            " - Position: " D-NUM4
                     DELIMITED BY SIZE INTO DATEN-BUFFER1
                     PERFORM Z002-PROGERR
                     MOVE 30           TO w-AC
                     EXIT SECTION

      END-EVALUATE
     END-PERFORM

     .
 C100-99.
      EXIT.


******************************************************************
* Insert auf Tabelle ASYNC70
******************************************************************
 S210-INSERT-ASYNC70 SECTION.
 S210-00.
     EXEC SQL
         INSERT
           INTO  =ASYNC70
                 ( PNR,       TERMNR,  TRACENR,  ISONTYP, MDNR,
                   TSNR,      VKZ,     CARDID,   ROUTKZ,  TRACENR_AS,
                   KZ_BEARB,  ANZ_REP, KEY_NAME, ZPINS,   ZPUPD,
                   FREHEADER, ANFRAGE, ANTWORT
                 )
         VALUES  (
                  :PNR        OF ASYNC70
                 ,:TERMNR     OF ASYNC70
                 ,:TRACENR    OF ASYNC70
                 ,:ISONTYP    OF ASYNC70
                 ,:MDNR       OF ASYNC70
                 ,:TSNR       OF ASYNC70
                 ,:VKZ        OF ASYNC70
                 ,:CARDID     OF ASYNC70
                 ,:ROUTKZ     OF ASYNC70
                 ,:TRACENR-AS OF ASYNC70
                 ,:KZ-BEARB   OF ASYNC70
                 ,:ANZ-REP    OF ASYNC70
                 ,:KEY-NAME   OF ASYNC70
                 ,current  YEAR TO FRACTION(2)
                 ,current  YEAR TO FRACTION(2)
                 ,:FREHEADER      OF ASYNC70
                 ,:ANFRAGE        OF ASYNC70
                 ,:ANTWORT        OF ASYNC70
                 )
     END-EXEC
     EVALUATE SQLCODE OF SQLCA
         WHEN ZERO   SET ASYNC70-OK  TO TRUE
         WHEN OTHER  SET ASYNC70-NOK TO TRUE
                     SET ENDE        TO TRUE
     END-EVALUATE
     .
 S210-99.
     EXIT.

******************************************************************
* Laden STIPLIMS
******************************************************************
 S600-GET-STIPLIMS SECTION.
 S600-00.

* Kartenwechsel in W-CARDID-OLD
    IF CARDID OF STIPLIMS = W-CARDID-OLD
       EXIT SECTION
    END-IF

    EXEC SQL
         SELECT  STIP_CALL    * 100,
                 STIP_REJECT  * 100
         INTO   :H-STIP-CALL,
                :H-STIP-REJECT
         FROM =STIPLIMS
         WHERE   CARDID,
                 ROUTKZ
              = :CARDID OF STIPLIMS,
                 0
         FOR BROWSE ACCESS
    END-EXEC

    IF SQLCODE OF SQLCA = ZERO
*Neue Cardid merken
       MOVE CARDID OF STIPLIMS TO W-CARDID-OLD
    ELSE
       INITIALIZE GEN-ERROR
       MOVE SQLCODE OF SQLCA TO D-NUM4
       STRING "Fehler bei Lesen =STIPLIMS ", D-NUM4
       DELIMITED BY SIZE
           INTO   DATEN-BUFFER1
       STRING "CARDID: ", CARDID OF STIPLIMS
       DELIMITED BY SIZE
           INTO   DATEN-BUFFER2
       PERFORM Z002-PROGERR

       IF SQLCODE OF SQLCA = -8300
*         Ausgabe SQLERROR nur, wenn "File System Error", sonst PROGERR
          PERFORM Z001-SQLERROR
       END-IF
       SET ENDE TO TRUE
     END-IF
     .
 S600-99.
     EXIT.

******************************************************************
* Indoor: Nur pruefen, wenn die Restriktionspruefung für Indoor
*         eingeschaltet ist - W-RST-PRUEFEN true, weil W-RST-FLAG = "J"
*********************************************************************
 S700-RESTRIKTIONEN-INDOOR SECTION.
 S700-00.

     MOVE ZEROS  TO WG        OF WGRSTPRF
     MOVE SPACES TO OUTDOOR   OF WGRSTPRF

     MOVE ZEROS  TO WG-TS     OF ARTMAP
     MOVE ZEROS  TO WG-HOST   OF ARTMAP
     MOVE SPACES TO M-UNIT    OF ARTMAP
     MOVE ZEROS  TO TAX-CODE  OF ARTMAP

     MOVE ZEROS TO UP-COUNTER2

     PERFORM
     VARYING C4-I1
        FROM 1 BY 1
       UNTIL C4-I1 > UP-COUNTER
          OR C4-I1 > 20
          OR ENDE
          OR W-AC NOT = ZEROS
             MOVE UP-PROD-CODE(C4-I1)
               TO WG-TS OF ARTMAP
             PERFORM S701-RESTRIKTIONEN-INDOOR
     END-PERFORM

     .
 S700-99.
     EXIT.

******************************************************************
* Indoor: Restriktionspruefung der TXn (Cardid, WG, RST-CODE)
* RST-CODE: Priorisierung der erlaubten Artikel in Menge
* ( Was und wieviel )
******************************************************************

 S701-RESTRIKTIONEN-INDOOR SECTION.
 S701-00.

     EXEC SQL
          SELECT  B.WG_TS
          INTO    :WG-TS      OF ARTMAP
          FROM   =WGRSTPRF A, =ARTMAP B
          WHERE  A.WG     = B.WG_HOST
            AND  A.CARDID = B.CARDID
            AND   B.MDNR,
                  B.TSNR,
                  B.CARDID,
                  B.WG_TS
              =  :MDNR      OF ARTMAP
                ,:TSNR      OF ARTMAP
                ,:CARDID    OF ARTMAP
                ,:WG-TS     OF ARTMAP
            AND   A.RST_CODE
              <= :RST-CODE OF WGRSTPRF
          FOR BROWSE ACCESS
     END-EXEC

     IF SQLCODE OF SQLCA = ZERO
*Produkt als erlaubtes Produkt merken in Rueckgabestruktur BMP63
        ADD    1                      TO UP-COUNTER2
        MOVE   WG-TS OF ARTMAP        TO UP-PROD-CODE2(UP-COUNTER2)
        EXIT SECTION
     ELSE
        IF SQLCODE OF SQLCA = 100
           CONTINUE
        ELSE
*Sonstiger SQL-Fehler = Nicht erlaubt + Meldung
           MOVE 87                     TO W-AC
           INITIALIZE GEN-ERROR
           MOVE SQLCODE OF SQLCA       TO D-NUM4
           STRING "Fehler bei Lesen =WGRSTPRF ", D-NUM4
           DELIMITED BY SIZE INTO DATEN-BUFFER1
           STRING "CARDID: ", CARDID   OF WGRSTPRF,
                  " / WG: ",  WG       OF WGRSTPRF,
                  " / RST: ", RST-CODE OF WGRSTPRF
           DELIMITED BY SIZE INTO DATEN-BUFFER2
           PERFORM Z002-PROGERR
           IF SQLCODE OF SQLCA = -8300
*Ausgabe SQLERROR nur, wenn "File System Error", sonst PROGERR
              PERFORM Z001-SQLERROR
           END-IF
           EXIT SECTION
        END-IF
     END-IF

**Zweiter Versuch: TSNR = 0 (bei Artikelmapping auf Mandantenebene)

     MOVE ZERO TO TSNR OF ARTMAP
     EXEC SQL
          SELECT B.WG_TS
          INTO   :WG-TS OF ARTMAP
          FROM   =WGRSTPRF A, =ARTMAP B
          WHERE  A.WG     = B.WG_HOST
            AND  A.CARDID = B.CARDID
          AND    B.MDNR,
                 B.TSNR,
                 B.CARDID,
                 B.WG_TS
              = :MDNR      OF ARTMAP
               ,:TSNR      OF ARTMAP
               ,:CARDID    OF ARTMAP
               ,:WG-TS     OF ARTMAP
          AND    A.RST_CODE
             <= :RST-CODE OF WGRSTPRF
          FOR BROWSE ACCESS
     END-EXEC

     IF SQLCODE OF SQLCA = ZERO
*Produkt als erlaubtes Produkt merken in Rueckgabestruktur BMP63
        ADD    1                      TO UP-COUNTER2
        MOVE   WG-TS OF ARTMAP        TO UP-PROD-CODE2(UP-COUNTER2)
     ELSE
        IF SQLCODE OF SQLCA = 100
*Nicht gefunden = Nicht erlaubt
           MOVE 87                     TO W-AC
        ELSE
*          Sonstiger SQL-Fehler = Nicht erlaubt + Meldung
           MOVE 87                     TO W-AC
           INITIALIZE GEN-ERROR
           MOVE SQLCODE OF SQLCA       TO D-NUM4
           STRING "Fehler bei Lesen =WGRSTPRF ", D-NUM4
           DELIMITED BY SIZE
                   INTO DATEN-BUFFER1
           STRING "CARDID: ", CARDID   OF WGRSTPRF,
                  " / WG: ",  WG       OF WGRSTPRF,
                  " / RST: ", RST-CODE OF WGRSTPRF
           DELIMITED BY SIZE
                   INTO DATEN-BUFFER2
           PERFORM Z002-PROGERR
           IF SQLCODE OF SQLCA = -8300
*Ausgabe SQLERROR nur, wenn "File System Error", sonst PROGERR
              PERFORM Z001-SQLERROR
           END-IF
        END-IF
     END-IF
     .
 S701-99.
     EXIT.

******************************************************************
* Restriktionspruefung fuer Outdoor-Transaktionen, die durch
* BMP 25 gesetzt im Bereich Anfrage-Check identifiziert wurden
******************************************************************
 S702-RESTRIKTIONEN-OUTDOOR SECTION.
 S702-00.

     MOVE ZEROS  TO WG        OF WGRSTPRF

     MOVE ZEROS  TO WG-TS     OF ARTMAP
     MOVE ZEROS  TO WG-HOST   OF ARTMAP
     MOVE ZEROS  TO M-UNIT    OF ARTMAP
     MOVE ZEROS  TO TAX-CODE  OF ARTMAP

*G.00.06 - Anfang
     MOVE ZEROS TO UP-COUNTER2
*G.00.06 - Ende

     EXEC SQL
        OPEN OUTDOOR_CURS
     END-EXEC
     IF SQLCODE OF SQLCA = ZEROS
        CONTINUE
     ELSE
        INITIALIZE GEN-ERROR
        MOVE SQLCODE OF SQLCA TO D-NUM4
        STRING "Fehler bei OPEN OUTDOOR_CURS: ", D-NUM4
        DELIMITED BY SIZE
                INTO DATEN-BUFFER1
        STRING "CARDID: ", CARDID   OF WGRSTPRF,
               " / RST: ", RST-CODE OF WGRSTPRF
        DELIMITED BY SIZE
                INTO DATEN-BUFFER2
        PERFORM Z002-PROGERR

        IF SQLCODE OF SQLCA = -8300
*Ausgabe SQLERROR nur, wenn "File System Error", sonst PROGERR
           PERFORM Z001-SQLERROR
        END-IF
     END-IF

     EXEC SQL
        FETCH OUTDOOR_CURS
        INTO  :H-WG-OUTDOOR
     END-EXEC

     PERFORM UNTIL SQLCODE OF SQLCA NOT = ZERO

*G.00.06 - Anfang
*               OR UP-COUNTER2 > 20
**
                OR UP-COUNTER2 > 19
*G.00.06 - Ende

        IF SQLCODE OF SQLCA = ZERO
*Produkt als erlaubtes Produkt merken in Rueckgabestruktur BMP63
           ADD    1                      TO UP-COUNTER2
           MOVE   H-WG-OUTDOOR           TO UP-PROD-CODE2(UP-COUNTER2)
        ELSE
           IF SQLCODE OF SQLCA = 100
              CONTINUE
           ELSE
*Sonstiger SQL-Fehler = Nicht erlaubt + Meldung
              INITIALIZE GEN-ERROR
              MOVE SQLCODE OF SQLCA TO D-NUM4
              STRING "Fehler bei Lesen =WGRSTPRF ", D-NUM4
              DELIMITED BY SIZE
                          INTO DATEN-BUFFER1
              STRING "CARDID: ", CARDID   OF WGRSTPRF,
                     " / RST: ", RST-CODE OF WGRSTPRF
              DELIMITED BY SIZE
                          INTO DATEN-BUFFER2
              PERFORM Z002-PROGERR

                          IF SQLCODE OF SQLCA = -8300
*Ausgabe SQLERROR nur, wenn "File System Error", sonst PROGERR
                 PERFORM Z001-SQLERROR
              END-IF
           END-IF
        END-IF

*NACHLESEN CURSOR
        EXEC SQL
           FETCH OUTDOOR_CURS
           INTO  :H-WG-OUTDOOR
        END-EXEC

     END-PERFORM

     EXEC SQL
        CLOSE OUTDOOR_CURS
     END-EXEC
     .
 S702-99.
     EXIT.

******************************************************************
* OPEN Cursor
******************************************************************
 S900-OPEN-FCPARAM-CURSOR SECTION.
 S900-00.
     MOVE ZERO TO C9-COUNT
     EXEC SQL
         OPEN FCPARAM_CURS
     END-EXEC
     .
 S900-99.
     EXIT.

******************************************************************
* Fetch aus Tabelle FCPARAM
******************************************************************
 S910-FETCH-FCPARAM-CURSOR SECTION.
 S910-00.
     EXEC SQL
         FETCH FCPARAM_CURS
          INTO  :ROUTKZ of FCPARAM
               ,:CARDID of FCPARAM
               ,:ISONTYP of FCPARAM
               ,:KZ-MSG of FCPARAM
               ,:BMP of FCPARAM
               ,:LFDNR of FCPARAM
               ,:KZ-ABWEICHUNG of FCPARAM
     END-EXEC
     EVALUATE SQLCODE OF SQLCA
         WHEN ZERO   SET FCPARAM-OK  TO TRUE
                     ADD 1 TO C9-COUNT
         WHEN 100    SET FCPARAM-EOD TO TRUE
         WHEN OTHER  SET FCPARAM-NOK TO TRUE
     END-EVALUATE
     .
 S910-99.
     EXIT.

******************************************************************
* CLOSE Cursor
******************************************************************
 S920-CLOSE-FCPARAM-CURSOR SECTION.
 S920-00.
     EXEC SQL
         CLOSE FCPARAM_CURS
     END-EXEC
     .
 S920-99.
     EXIT.

******************************************************************
* OPEN Cursor
******************************************************************
 S930-OPEN-KEYNAMEN-CURSOR SECTION.
 S930-00.
     MOVE ZERO TO C9-COUNT
     EXEC SQL
         OPEN KEYNAMEN_CURS
     END-EXEC
     .
 S930-99.
     EXIT.

******************************************************************
* Fetch aus Tabelle KEYNAMEN
******************************************************************
 S940-FETCH-KEYNAMEN-CURSOR SECTION.
 S940-00.
     EXEC SQL
         FETCH KEYNAMEN_CURS
          INTO  :ROUTKZ of KEYNAMEN
               ,:CARDID of KEYNAMEN
               ,:KEYNAME of KEYNAMEN
               ,:ISOGEN of KEYNAMEN
               ,:ISOVERS of KEYNAMEN
     END-EXEC
     EVALUATE SQLCODE OF SQLCA
         WHEN 0      SET KEYNAMEN-OK  TO TRUE
                     ADD 1 TO C9-COUNT
         WHEN OTHER  SET KEYNAMEN-NOK TO TRUE
     END-EVALUATE
     .
 S940-99.
     EXIT.

******************************************************************
* CLOSE Cursor
******************************************************************
 S950-CLOSE-KEYNAMEN-CURSOR SECTION.
 S950-00.
     EXEC SQL
         CLOSE KEYNAMEN_CURS
     END-EXEC
     .
 S950-99.
     EXIT.
     
******************************************************************
* Select auf neue AIID-Tabelle
*G.02.01 - neu
******************************************************************
 S960-SELECT-AIID SECTION.
 S960-00.

     EXEC SQL
         SELECT AIID
         INTO  :AIID     OF FCAIID
         FROM  =FCAIID
         WHERE ROUTKZ, CARDID =
              :ROUTKZ    OF KEYNAMEN
             ,:CARDID    OF KEYNAMEN
         BROWSE ACCESS
     END-EXEC
     
     
     IF SQLCODE OF SQLCA = 100
         EXEC SQL
             SELECT AIID
             INTO  :AIID     OF FCAIID
             FROM  =FCAIID
             WHERE ROUTKZ, CARDID =
                  :ROUTKZ    OF KEYNAMEN
                 ,0
             BROWSE ACCESS
         END-EXEC
     END-IF
     
     EVALUATE SQLCODE OF SQLCA
        WHEN 0      CONTINUE
** ---> Dummy AIID, fuer RoutKZ, die nicht gepflegt sind
        WHEN 100    MOVE "000000" TO AIID OF FCAIID
        WHEN OTHER  MOVE  SQLCODE OF SQLCA    TO D-NUM4
                    STRING "Fehler bei LESEN FCAIID: ", D-NUM4
                    DELIMITED BY SIZE INTO DATEN-BUFFER1
                    MOVE ROUTKZ OF KEYNAMEN TO D-NUM4M
                    MOVE CARDID OF KEYNAMEN TO D-NUM4N
                    STRING "ROUTKZ= ",
                            D-NUM4M,
                            " / CARDID= ",
                            D-NUM4N,
                            " oder 0"
                    DELIMITED BY SIZE INTO DATEN-BUFFER2
                    PERFORM Z002-PROGERR
     END-EVALUATE

     .
 S960-99.
     EXIT.

